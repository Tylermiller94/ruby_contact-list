<h2><%= @contact.first_name %></h2>
<h2><%= @contact.last_name %></h2>
<h2><%= @contact.job_title %></h2>
<h2><%= @contact.company %></h2>
<h2><%= @contact.contact_type%></h2>
<h2><%= @contact.street_address%></h2>
<h2><%= @contact.city%></h2>
<h2><%= @contact.state%></h2>
<h2><%= @contact.zip%></h2>
<h2><%= @contact.phone_number%></h2>
<br>
<br>
<br>
<br>
<form action="/contacts/<%= @contact.last_name%>" method="post">
  <div class="form-group">
    <label for="last_name">Last Name</label>
    <input id="last_name" name="last_name" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="new_phone_number">New Phone Number</label>
    <input id="new_phone_number" name="new_phone_number" class="form-control" type="text">
  </div>
  <button type="submit" class="btn">Add!</button>
</form>


